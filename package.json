{
  "name": "qas-highlighter",
  "displayName": "qas-highlighter",
  "publisher": "h1u2i3",
  "description": "Graphql Api Server config file.",
  "version": "0.0.2",
  "icon": "images/logo.png",
  "engines": {
    "vscode": "^1.35.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/h1u2i3/qas-highlighter.git"
  },
  "contributes": {
    "languages": [
      {
        "id": "qas",
        "aliases": [
          "qas",
          "Qas"
        ],
        "extensions": [
          ".qas"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "qas",
        "scopeName": "source.qas",
        "path": "./syntaxes/qas.tmLanguage.json",
        "embeddedLanguages": {
          "meta.embedded.block.elixir": "elixir"
        }
      }
    ]
  }
}
